# APPS #
#
# Arch
alt - a : run-external "open" "-a" `/Applications/Arc.app/Contents/MacOS/Arc`

# Chatgpt
alt - c : run-external "open" "-a" `/Applications/Chatgpt.app/Contents/MacOS/Chatgpt`

# Show date
alt - d : osascript ~/.config/skhd/applescripts/pop.scpt

# Goland
lalt - g : run-external "open" "-a" `~/Applications/Goland.app/Contents/MacOS/goland`

# Ghostty Terminal
ralt - g : run-external "open" "-a" `/Applications/Ghostty.app/Contents/MacOS/ghostty`

# Close notifications
ralt - n : osascript ~/.config/skhd/applescripts/notifications.scpt

# Obsidian
alt - o : run-external "open" "-a" `/Applications/Obsidian.app/Contents/MacOS/Obsidian`

# QuickTime
alt - q : run-external "open" "-a" `/System/Applications/QuickTime\ Player.app/Contents/MacOS/QuickTime\ Player`

# Slack
lalt - s : run-external "open" "-a" `/Applications/Slack.app/Contents/MacOS/Slack`

# Signal
ralt - s : run-external "open" "-a" `/Applications/Signal.app/Contents/MacOS/Signal`

# Telegram
alt - t : run-external "open" "-a" `/Applications/Telegram.app/Contents/MacOS/Telegram`

# Visual Studio Code
alt - v : run-external "open" "-a" `/Applications/Visual\ Studio\ Code.app/Contents/MacOS/Electron`

# Wezterm
lalt - w : run-external "open" "-a" `/Applications/Wezterm.app/Contents/MacOS/wezterm`

# Whatsapp
ralt - w : run-external "open" "-a" `/Applications/WhatsApp.app/Contents/MacOS/WhatsApp`

# Zed
alt - z : run-external "open" "-a" `/Applications/Zed.app/Contents/MacOS/zed`
